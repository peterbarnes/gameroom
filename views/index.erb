
	<div id = "content">
		<div id = "feature">
      <% @widgets.each do |widget| %>
  			<p><%= widget.id %></p>
        <p><%= widget.name %></p>
        <p><%= widget.created_at %></p>
        <p><%= widget.updated_at %></p>
        <p><%= widget.data %></p>
        <a href = "/delete?id=<%= widget.id %>" class = "del">Delete</a>
      <% end %>
      <form action="/create" method= "post">
        <input name="name" type="text" />
        <input type="submit" value="Save" />
      </form>
		</div>
