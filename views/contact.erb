<script>
$(document).ready(function() {
  $('.details').hide();
  $('.main h2').click(function() {
    $('.details').toggle(
      function() {
        $(this).next('.details').slideDown();
      },
      function() {
        $(this).next('.details').slideUp();
      }
    );//end toggle
  });//end click
});//end ready
</script>
